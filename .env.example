# Токен телеграм бота, можно получить при создании бота в @botfather
TOKEN=123456789:ABCDEFGHIJKLMnopqrstuvwxyz12345678

# Название категории для хранения логов, по умолчанию: logs
LOG_DIR=logs
# Название файла с логами, по умолчанию: tg_you_can.log
LOG_FILE_NAME=tg_you_can.log
# Уровень логирования, доступные: DEBUG, INFO, WARNING, ERROR, CRITICAL
# По умолчанию: INFO
LOG_LEVEL=INFO

# Настройка DEBUG для Django
DEBUG=True
SECRET_KEY=django-secret-key
ALLOWED_HOSTS=

# Если используется другая СУБД, то строку ниже следует закомментировать
NEED_SQLITE=True

# Настройки подключения
DB_ENGINE=django.db.backends.postgresql_psycopg2
POSTGRES_DB=postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
DB_HOST=db
DB_PORT=5432
